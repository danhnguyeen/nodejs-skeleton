{
  "extends": "airbnb-base",
  "env": {
      "es6": true,
      "browser": true
  },
  "rules": {
      "brace-style": [
          "error",
          "1tbs"
      ],
      "comma-dangle": [
          "error",
          "never"
      ],
      "import/named": 0,
      "func-names": 0,
      "consistent-return": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 0
  }
}